                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                           <children>
                              <Button id="drinkButton" mnemonicParsing="false" stylesheets="@../gui/combinedStyle.css" text="Water" textFill="#010006" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                                 <font>
                                    <Font name="System Bold" size="13.0" />
                                 </font>
                              </Button>
                           </children>
                        </GridPane>
                     </content>
