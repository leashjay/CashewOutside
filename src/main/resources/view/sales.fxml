                                    </rowConstraints>
                                    <children>
                                        <Button id="foodButton" mnemonicParsing="false" stylesheets="@../gui/combinedStyle.css" text="Beef" textFill="#010006" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                                            <font>
                                                <Font name="System Bold" size="13.0" />
                                            </font>
                                        </Button>
                                    </children>
                                </GridPane>
                            </content>
                        </ScrollPane>
                    </content>
                </Tab>
                <Tab text="Drinks">
                    <content>
                        <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" vbarPolicy="ALWAYS">
                     <content>
                        <GridPane fx:id="drinkItemGrid" prefHeight="600.0" prefWidth="950.0" snapToPixel="false" stylesheets="@../gui/combinedStyle.css">
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
