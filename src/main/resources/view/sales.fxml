                        <ScrollPane hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" pannable="true" prefHeight="200.0" prefWidth="200.0" vbarPolicy="ALWAYS">
                            <content>
                                <GridPane fx:id="foodItemGrid" centerShape="false" maxHeight="500000.0" maxWidth="950.0" minHeight="600.0" minWidth="950.0" snapToPixel="false" stylesheets="@../gui/combinedStyle.css">
